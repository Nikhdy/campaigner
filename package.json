{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "CEX",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --output-public-path=/assets/",
    "clean": "rimraf ../public/public",
    "clean:dll": "rimraf ../public/dll/",
    "build:dll": "npm run clean:dll && webpack --config webpack.dll.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "typings": "rimraf typings/ && typings install",
    "stats:dev": "NODE_ENV=development webpacPk --profile --json > wpstats.json"
  },
  "license": "MIT",
